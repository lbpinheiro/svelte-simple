{
	"name": "svelte-simple",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"packageManager": "pnpm@9.15.3",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
